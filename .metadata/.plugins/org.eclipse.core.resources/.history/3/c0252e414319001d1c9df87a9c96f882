package com.tcs;

import java.util.ArrayList;
import java.util.List;

public  class IPhone   {
	
 	
	public static void main (String[] args) {
		 
	List emp = new ArrayList();
	emp.add("Nagul");
	emp.add('a');
	emp.add(99.6);
	emp.add(true);
	emp.add(200);
	System.out.println(emp);
	Object n = emp.get(2);
	System.out.println(n);
	int y = emp.size();
	System.out.println(y);
	for(int m=0; m<y;m++) {
		Object s = emp.get(m);
		System.out.println(s);
	}
	
	
	
	
		
		
			
		}
		
	} 

 
	
	

